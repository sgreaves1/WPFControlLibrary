<Page x:Class="LibrarySamples.Pages.SelectiveList.SelectiveListPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LibrarySamples.Pages.SelectiveList"
      xmlns:myLibrary="clr-namespace:MyLibrary;assembly=MyLibrary"
      mc:Ignorable="d" 
      Title="SelectiveListPage">

    <Grid>
        <myLibrary:SelectiveListUc AvailibleItems="{Binding Path=AvailibleItems}"
                                   SelectedItems="{Binding Path=SelectedItems}"
                                   Colour="SkyBlue"/>
    </Grid>
</Page>
